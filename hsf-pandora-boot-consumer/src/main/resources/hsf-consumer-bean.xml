<?xml version="1.0" encoding="GB2312"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-autowire="byName">

    <bean id="helloService" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean">
        <property name="interfaceName" value="com.alibaba.edas.HelloService"/>
        <property name="version" value="${hsf.version}"/>
        <property name="clientTimeout" value="${hsf.timeout}"/>
    </bean>


    <bean id="pointService" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean">
        <property name="interfaceName" value="com.alibaba.edas.PointService"/>
        <property name="version" value="${hsf.version}"/>
        <property name="clientTimeout" value="${hsf.timeout}"/>
    </bean>

</beans>